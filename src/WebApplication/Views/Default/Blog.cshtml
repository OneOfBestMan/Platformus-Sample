@model dynamic
@{ 
  this.ViewBag.Title = this.Model.Properties["Title"].Html;
  this.ViewBag.MetaKeywords = this.Model.Properties["MetaKeywords"].Html;
  this.ViewBag.MetaDescription = this.Model.Properties["MetaDescription"].Html;
}

@Html.Raw(this.Model.Properties["Content"].Html)

@foreach (var blogPost in this.Model.DataSources["BlogPosts"].Objects)
{
  @Html.Partial("_BlogPost", blogPost as object)
}